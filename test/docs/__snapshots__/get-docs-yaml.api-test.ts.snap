exports[`GET /docs-yaml matches the snapshot 1`] = `
"openapi: 3.0.0
paths:
  /api/v1/test:
    get:
      operationId: TestController_getToken
      parameters: []
      responses:
        '200':
          description: ''
  /api/v1/deals/{dealIdentifier}:
    get:
      operationId: DealController_getDealByIdentifier
      summary: Get the deal matching the specified deal identifier.
      parameters:
        - name: dealIdentifier
          required: true
          in: path
          description: The identifier of the deal in ACBS.
          example: '0030000321'
          schema:
            type: string
      responses:
        '200':
          description: The deal has been successfully retrieved.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/GetDealByIdentifierResponse'
        '404':
          description: The specified deal was not found.
        '500':
          description: An internal server error has occurred.
  /api/v1/deals:
    post:
      operationId: DealController_createDeal
      summary: Creates a new deal.
      parameters: []
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: array
              items:
                $ref: '#/components/schemas/CreateDealRequestItem'
      responses:
        '201':
          description: The deal has been successfully created.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CreateDealResponse'
        '400':
          description: Bad request.
        '500':
          description: An internal server error has occurred.
  /api/v1/deals/{dealIdentifier}/guarantees:
    post:
      operationId: DealGuaranteeController_createGuaranteeForDeal
      summary: Create a new guarantee for a deal.
      parameters:
        - name: dealIdentifier
          required: true
          in: path
          description: The identifier of the deal in ACBS.
          example: '0030000321'
          schema:
            type: string
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: array
              items:
                $ref: '#/components/schemas/CreateDealGuaranteeRequestItem'
      responses:
        '201':
          description: The guarantee has been successfully created.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CreateDealGuaranteeResponse'
        '400':
          description: Bad request.
        '404':
          description: The deal was not found.
        '500':
          description: An internal server error has occurred.
    get:
      operationId: DealGuaranteeController_getGuaranteesForDeal
      summary: Get all guarantees for a specific deal.
      parameters:
        - name: dealIdentifier
          required: true
          in: path
          description: The identifier of the deal in ACBS.
          example: '0030000321'
          schema:
            type: string
      responses:
        '200':
          description: The deal guarantees have been successfully retrieved.
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/GetDealGuaranteeResponseItem'
        '204':
          description: The deal has no guarantees.
        '400':
          description: Bad request.
        '404':
          description: The deal was not found.
        '500':
          description: An internal server error has occurred.
  /api/v1/deals/{dealIdentifier}/investors:
    get:
      operationId: DealInvestorController_getDealInvestors
      summary: Get all investors of specific deal.
      parameters:
        - name: dealIdentifier
          required: true
          in: path
          description: UKEF id for deal
          example: '0030000321'
          schema:
            type: string
      responses:
        '200':
          description: The deal investors have been successfully retrieved.
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/GetDealInvestorResponseDto'
        '404':
          description: The specified deal, or the investors for that deal, were not found.
        '500':
          description: An internal server error has occurred.
    post:
      operationId: DealInvestorController_createInvestorForDeal
      summary: Create a new investor for a deal.
      parameters:
        - name: dealIdentifier
          required: true
          in: path
          description: The identifier of the deal in ACBS.
          example: '0020900111'
          schema:
            type: string
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: array
              items:
                $ref: '#/components/schemas/CreateDealInvestorRequestItem'
      responses:
        '201':
          description: The investor has been successfully created.
        '400':
          description: Bad request.
        '404':
          description: The deal was not found.
        '500':
          description: An internal server error has occurred.
  /api/v1/facilities/{facilityIdentifier}/investors:
    post:
      operationId: FacilityInvestorController_createInvestorForFacility
      summary: Create a new investor for a facility.
      parameters:
        - name: facilityIdentifier
          required: true
          in: path
          description: The identifier of the facility in ACBS.
          example: '0000000001'
          schema:
            type: string
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: array
              items:
                $ref: '#/components/schemas/CreateFacilityInvestorRequestItem'
      responses:
        '201':
          description: The investor has been successfully created.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CreateFacilityInvestorResponse'
        '400':
          description: Bad request.
        '404':
          description: The facility was not found.
        '500':
          description: An internal server error has occurred.
  /api/v1/parties/{partyIdentifier}/external-ratings:
    get:
      operationId: PartyExternalRatingController_getExternalRatingsForParty
      summary: Get all external ratings for a party.
      parameters:
        - name: partyIdentifier
          required: true
          in: path
          description: The identifier of the party in ACBS.
          example: '00000001'
          schema:
            type: string
      responses:
        '200':
          description: The external ratings for the party have been successfully retrieved.
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/GetPartyExternalRatingsResponseItem'
        '404':
          description: >-
            The specified party, or the external ratings for that party, were
            not found.
        '500':
          description: An internal server error has occurred.
  /api/v1/parties:
    get:
      operationId: PartyController_getPartiesBySearchText
      summary: Get all parties matching the specified search text.
      parameters:
        - name: searchText
          required: true
          in: query
          description: 'Minimum length: 3. Cannot contain only whitespaces.'
          schema:
            minLength: 3
            type: string
      responses:
        '200':
          description: The matching parties have been successfully retrieved.
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/GetPartiesBySearchTextResponseItem'
        '500':
          description: An internal server error has occurred.
    post:
      operationId: PartyController_createParty
      summary: >-
        Create a new party if there are no parties matching this alternate
        identifier.
      parameters: []
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: array
              items:
                $ref: '#/components/schemas/CreatePartyRequestItem'
      responses:
        '200':
          description: >-
            One or more parties matching this alternate identifier already
            exist.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CreatePartyResponse'
        '201':
          description: The party has been successfully created.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CreatePartyResponse'
        '400':
          description: Bad request.
        '500':
          description: An internal server error has occurred.
  /api/v1/parties/{partyIdentifier}:
    get:
      operationId: PartyController_getPartyByIdentifier
      summary: Get the party matching the specified party identifier.
      parameters:
        - name: partyIdentifier
          required: true
          in: path
          description: The identifier of the party in ACBS.
          example: '00000001'
          schema:
            type: string
      responses:
        '200':
          description: The party has been successfully retrieved.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/GetPartyByIdentifierResponse'
        '404':
          description: The specified party was not found.
        '500':
          description: An internal server error has occurred.
info:
  title: TFS API Specification
  description: TFS API documentation
  version: '1.0'
  contact: {}
tags: []
servers: []
components:
  securitySchemes:
    ApiKeyHeader:
      type: apiKey
      in: header
      name: x-api-key
  schemas:
    GetDealByIdentifierResponse:
      type: object
      properties:
        dealIdentifier:
          type: string
          readOnly: true
          example: '0030000321'
        portfolioIdentifier:
          type: string
          readOnly: true
          example: E1
        currency:
          type: string
          readOnly: true
          example: USD
        dealValue:
          type: number
          readOnly: true
        guaranteeCommencementDate:
          format: date
          type: string
          readOnly: true
        obligorPartyIdentifier:
          type: string
          readOnly: true
          example: '00000001'
        obligorName:
          type: string
          readOnly: true
        obligorIndustryClassification:
          type: string
          readOnly: true
      required:
        - dealIdentifier
        - portfolioIdentifier
        - currency
        - dealValue
        - guaranteeCommencementDate
        - obligorPartyIdentifier
        - obligorName
        - obligorIndustryClassification
    CreateDealRequestItem:
      type: object
      properties:
        dealIdentifier:
          type: string
          description: The identifier of the deal to create.
          minLength: 10
          maxLength: 10
          example: '0020900035'
        currency:
          type: string
          description: >-
            The currency code of the primary currency of the deal, from the
            Currency Definition Table.
          minLength: 3
          maxLength: 3
          example: GBP
        dealValue:
          type: number
          description: The value of the deal.
          minimum: 0
        guaranteeCommencementDate:
          format: date
          type: string
          description: >-
            Deal effective date is not in workflow. Currently operations assume
            a date earlier than the Facility effective date. Use the earliest
            Effective Date on facilities in Workflow under this Deal, i.e.
            Guarantee Commencement Date.
        obligorPartyIdentifier:
          type: string
          description: The obligor party identifier.
          minLength: 8
          maxLength: 8
          example: '00000001'
        obligorName:
          type: string
          description: The obligor party name, which is party name1 attribute.
          minLength: 0
          maxLength: 35
        obligorIndustryClassification:
          type: string
          description: The obligor party industry classification.
          minLength: 0
          maxLength: 10
          example: '1405'
      required:
        - dealIdentifier
        - currency
        - dealValue
        - guaranteeCommencementDate
        - obligorPartyIdentifier
        - obligorName
        - obligorIndustryClassification
    CreateDealResponse:
      type: object
      properties:
        dealIdentifier:
          type: string
          example: '0020900035'
      required:
        - dealIdentifier
    CreateDealGuaranteeRequestItem:
      type: object
      properties:
        dealIdentifier:
          type: string
          description: The identifier of the deal to create the guarantee for.
          minLength: 8
          maxLength: 8
          example: '00000001'
        effectiveDate:
          format: date
          type: string
          description: >-
            The date that this guarantee will take effect. This will be replaced
            by today's date if a date in the past is provided.
        limitKey:
          type: string
          description: An ACBS party identifier.
          minLength: 8
          maxLength: 8
          example: '00000002'
        guaranteeExpiryDate:
          format: date
          type: string
          description: The date that this guarantee will expire on.
        maximumLiability:
          type: number
          description: The maximum amount the guarantor will guarantee.
          minimum: 0
        guarantorParty:
          type: string
          description: >-
            The party identifier of the guarantor, the customer who is making
            the guarantee/obligation.
          minLength: 8
          maxLength: 8
          default: '00000141'
        guaranteeTypeCode:
          type: string
          description: The identifier for the type of the guarantee.
          minLength: 3
          maxLength: 3
          default: '450'
      required:
        - dealIdentifier
        - effectiveDate
        - limitKey
        - guaranteeExpiryDate
        - maximumLiability
    CreateDealGuaranteeResponse:
      type: object
      properties:
        dealIdentifier:
          type: string
          readOnly: true
          example: '00000001'
      required:
        - dealIdentifier
    GetDealGuaranteeResponseItem:
      type: object
      properties:
        portfolioIdentifier:
          type: string
          example: E1
        dealIdentifier:
          type: string
          example: '0030000321'
        effectiveDate:
          format: date
          type: string
          description: The date that this guarantee will take effect.
        guarantorParty:
          type: string
          description: UK GOODS (00000141)
          minLength: 8
          maxLength: 8
          default: '00000141'
        limitKey:
          type: string
          description: An ACBS party identifier.
          minLength: 8
          maxLength: 8
          example: '00000002'
        guaranteeExpiryDate:
          format: date
          type: string
          description: The date that this guarantee will expire on.
        maximumLiability:
          type: number
          description: The maximum amount the guarantor will guarantee.
          minimum: 0
        guaranteeTypeCode:
          type: string
          description: GOODS (450)
          default: '450'
          minLength: 1
      required:
        - portfolioIdentifier
        - dealIdentifier
        - effectiveDate
        - limitKey
        - guaranteeExpiryDate
        - maximumLiability
    GetDealInvestorDtoLenderType:
      type: object
      properties:
        LenderTypeCode:
          type: string
          description: 'Lender type: 100 for Exporter or 500 for UKEF record.'
          example: '500'
      required:
        - LenderTypeCode
    GetDealInvestorResponseDto:
      type: object
      properties:
        dealIdentifier:
          type: string
          readOnly: true
          example: '0030000321'
        portfolioIdentifier:
          type: string
          readOnly: true
          example: E1
        lenderType:
          readOnly: true
          allOf:
            - $ref: '#/components/schemas/GetDealInvestorDtoLenderType'
        effectiveDate:
          format: date-time
          type: string
          nullable: false
        expiryDate:
          format: date-time
          type: string
          nullable: true
        isExpiryDateMaximum:
          type: boolean
          readOnly: true
          example: false
        maximumLiability:
          type: number
          readOnly: true
          example: 500012.25
      required:
        - dealIdentifier
        - portfolioIdentifier
        - lenderType
        - effectiveDate
        - expiryDate
        - isExpiryDateMaximum
        - maximumLiability
    CreateDealInvestorRequestItem:
      type: object
      properties:
        dealIdentifier:
          type: string
          description: >-
            'The identifier of the deal to create the investor for. It will be a
            10-digit code beginning with either '0020', '0030', or '0040'.
          minLength: 10
          maxLength: 10
          pattern: /00\\d{8}/
          example: '0020900111'
        lenderType:
          type: string
          description: The lender type code for the investor party of the deal.
          minLength: 0
          maxLength: 3
          example: '500'
          default: '500'
        effectiveDate:
          format: date
          type: string
          description: >-
            The effective date on the deal investor record. If the date provided
            is in the future, it will be replaced by today's date.
          example: '2023-03-24'
        expiryDate:
          format: date
          type: string
          description: >-
            The expiry date on the deal investor record. If the value is not
            provided or is null then the maximum expiry date will be set.
          example: '2023-03-24'
          default: null
        dealStatus:
          type: string
          description: >-
            A code for the status of the deal on which the sub-limit is created,
            e.g., 'A' for 'ACTIVE/PENDING', 'B' for 'ACTIVE' etc.
          minLength: 0
          maxLength: 1
          example: A
          default: A
        currency:
          type: string
          description: The deal currency code.
          minLength: 3
          maxLength: 3
          example: USD
      required:
        - dealIdentifier
        - effectiveDate
        - expiryDate
        - currency
    CreateFacilityInvestorRequestItem:
      type: object
      properties:
        facilityIdentifier:
          type: string
          description: The identifier of the facility to create the investor for.
          minLength: 10
          maxLength: 10
          example: '0000000001'
        effectiveDate:
          format: date
          type: string
          description: The date from which this limit is effective.
        guaranteeExpiryDate:
          format: date
          type: string
          description: The date on which this limit will expire.
        currency:
          type: string
          description: >-
            The code of the currency for this investor, Currency in the Currency
            Definition Table.
          minLength: 3
          maxLength: 3
          example: USD
        maximumLiability:
          type: number
          description: The investor's share of the current limit amount.
          minimum: 0
        lenderType:
          type: string
          description: >-
            The lender type for this investor, Key Value 1 from the T1300 -
            Lender Type Code Table.
          minLength: 3
          maxLength: 3
          default: '500'
      required:
        - facilityIdentifier
        - effectiveDate
        - guaranteeExpiryDate
        - currency
        - maximumLiability
    CreateFacilityInvestorResponse:
      type: object
      properties:
        facilityIdentifier:
          type: string
          readOnly: true
          minLength: 10
          maxLength: 10
          description: The identifier of the facility that the investor was created for.
          example: '0000000001'
      required:
        - facilityIdentifier
    GetPartyExternalRatingResponseRatingEntity:
      type: object
      properties:
        ratingEntityCode:
          type: string
          readOnly: true
      required:
        - ratingEntityCode
    GetPartyExternalRatingResponseAssignedRating:
      type: object
      properties:
        assignedRatingCode:
          type: string
          readOnly: true
      required:
        - assignedRatingCode
    GetPartyExternalRatingsResponseItem:
      type: object
      properties:
        partyIdentifier:
          type: string
          readOnly: true
        ratingEntity:
          readOnly: true
          allOf:
            - $ref: '#/components/schemas/GetPartyExternalRatingResponseRatingEntity'
        assignedRating:
          readOnly: true
          allOf:
            - $ref: >-
                #/components/schemas/GetPartyExternalRatingResponseAssignedRating
        ratedDate:
          format: date-time
          type: string
          readOnly: true
        probabilityofDefault:
          type: number
          readOnly: true
        lossGivenDefault:
          type: number
          readOnly: true
        riskWeighting:
          type: number
          readOnly: true
        externalRatingNote1:
          type: string
          readOnly: true
        externalRatingNote2:
          type: string
          readOnly: true
        externalRatingUserCode1:
          type: string
          readOnly: true
        externalRatingUserCode2:
          type: string
          readOnly: true
      required:
        - partyIdentifier
        - ratingEntity
        - assignedRating
        - ratedDate
        - probabilityofDefault
        - lossGivenDefault
        - riskWeighting
        - externalRatingNote1
        - externalRatingNote2
        - externalRatingUserCode1
        - externalRatingUserCode2
    GetPartiesBySearchTextResponseItem:
      type: object
      properties:
        alternateIdentifier:
          type: string
          readOnly: true
        industryClassification:
          type: string
          readOnly: true
        name1:
          type: string
          readOnly: true
        name2:
          type: string
          readOnly: true
        name3:
          type: string
          readOnly: true
        smeType:
          type: string
          readOnly: true
        citizenshipClass:
          type: string
          readOnly: true
        officerRiskDate:
          type: string
          readOnly: true
        countryCode:
          type: string
          readOnly: true
      required:
        - alternateIdentifier
        - industryClassification
        - name1
        - name2
        - name3
        - smeType
        - citizenshipClass
        - officerRiskDate
        - countryCode
    CreatePartyRequestItem:
      type: object
      properties:
        alternateIdentifier:
          type: string
          description: The UKEF ID for the party. Should contain just digits.
          minLength: 8
          maxLength: 8
          example: '00291013'
          pattern: /^d{8}$/
        industryClassification:
          type: string
          description: The primary industry classification code for this customer.
          minLength: 1
          maxLength: 10
          example: '0001'
        name1:
          type: string
          description: The primary customer name.
          minLength: 1
          maxLength: 35
          example: ACTUAL IMPORT EXPORT
        name2:
          type: string
          description: The secondary customer name.
          minLength: 1
          maxLength: 35
          example: ACTUAL IMPORT EXPORT
        name3:
          type: string
          description: The tertiary customer name.
          minLength: 1
          maxLength: 35
          example: ACTUAL IMPORT EXPORT
        smeType:
          type: string
          description: A code that indicates what minority class this customer represents.
          minLength: 1
          maxLength: 2
          example: '70'
        citizenshipClass:
          type: string
          description: >-
            A code that identifies the citizenship category of this customer.
            Should be '1' if the domicile country is the UK, otherwise '2'.
          example: '2'
        officerRiskDate:
          format: date
          type: string
          description: The date of creation in YYYY-MM-DD (ISO 8601) format.
          example: '2023-03-15'
        countryCode:
          type: string
          description: The country code for the party's primary address.
          maxLength: 3
          default: GBR
          example: DZA
      required:
        - alternateIdentifier
        - industryClassification
        - name1
        - smeType
        - citizenshipClass
        - officerRiskDate
    CreatePartyResponse:
      type: object
      properties:
        partyIdentifier:
          type: string
          readOnly: true
    GetPartyByIdentifierResponse:
      type: object
      properties:
        alternateIdentifier:
          type: string
          readOnly: true
        industryClassification:
          type: string
          readOnly: true
        name1:
          type: string
          readOnly: true
        name2:
          type: string
          readOnly: true
        name3:
          type: string
          readOnly: true
        smeType:
          type: string
          readOnly: true
        citizenshipClass:
          type: string
          readOnly: true
        officerRiskDate:
          format: date-time
          type: string
          readOnly: true
          nullable: true
        countryCode:
          type: string
          readOnly: true
      required:
        - alternateIdentifier
        - industryClassification
        - name1
        - name2
        - name3
        - smeType
        - citizenshipClass
        - officerRiskDate
        - countryCode
security:
  - ApiKeyHeader: []
"
`;
