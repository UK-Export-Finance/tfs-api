      operationId: PartyController_createParty
      summary: >-
        Create a new party if there are no parties matching this alternate
        identifier.
      parameters: []
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: array
              items:
                $ref: '#/components/schemas/CreatePartyRequestItem'
      responses:
        '200':
          description: >-
            One or more parties matching this alternate identifier already
            exist.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CreatePartyResponse'
        '201':
          description: The party has been successfully created.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CreatePartyResponse'
        '400':
          description: Bad request.
        '500':
          description: An internal server error has occurred.
  /api/v1/parties/{partyIdentifier}:
    get:
      operationId: PartyController_getPartyByIdentifier
      summary: Get the party matching the specified party identifier.
      parameters:
        - name: partyIdentifier
          required: true
          in: path
          description: The identifier of the party in ACBS.
          example: '00000001'
          schema:
            type: string
      responses:
        '200':
          description: The party has been successfully retrieved.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/GetPartyByIdentifierResponseDto'
        '404':
          description: The specified party was not found.
        '500':
          description: An internal server error has occurred.
  /api/v1/facilities/{facilityIdentifier}/loan-transactions/{bundleIdentifier}:
    get:
      operationId: FacilityLoanTransactionController_getLoanTransactionByBundleIdentifier
      summary: Get the loan transaction matching the specified bundle identifier.
      parameters:
        - name: facilityIdentifier
          required: true
          in: path
          description: The identifier of the facility in ACBS.
          example: '0030000322'
          schema:
            minLength: 10
            maxLength: 10
            pattern: ^00\\d{8}$
            type: string
        - name: bundleIdentifier
          required: true
          in: path
          description: The bundle identifier of the loan transaction in ACBS.
          example: '0000321000'
          schema:
            minLength: 10
            maxLength: 10
            pattern: ^0{4}\\d{6}$
            type: string
      responses:
        '200':
          description: The loan transaction has been successfully retrieved.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/GetFacilityLoanTransactionResponseDto'
        '404':
          description: The specified loan transaction was not found.
        '500':
          description: An internal server error has occurred.
info:
  title: TFS API Specification
  description: TFS API documentation
  version: '1.0'
  contact: {}
tags: []
servers: []
components:
  securitySchemes:
    ApiKeyHeader:
      type: apiKey
      in: header
      name: x-api-key
  schemas:
    GetDealByIdentifierResponse:
      type: object
      properties:
        dealIdentifier:
          type: string
          readOnly: true
          example: '0030000321'
        portfolioIdentifier:
          type: string
          readOnly: true
          example: E1
        currency:
          type: string
          readOnly: true
          example: USD
        dealValue:
          type: number
          readOnly: true
        guaranteeCommencementDate:
          format: date
          type: string
          readOnly: true
        obligorPartyIdentifier:
          type: string
          readOnly: true
          example: '00001234'
        obligorName:
          type: string
          readOnly: true
        obligorIndustryClassification:
          type: string
          readOnly: true
      required:
        - dealIdentifier
        - portfolioIdentifier
        - currency
        - dealValue
        - guaranteeCommencementDate
        - obligorPartyIdentifier
        - obligorName
        - obligorIndustryClassification
    CreateDealRequestItem:
      type: object
      properties:
        dealIdentifier:
          type: string
          description: The identifier of the deal to create.
          minLength: 10
          maxLength: 10
          example: '0020900035'
        currency:
          type: string
          description: >-
            The currency code of the primary currency of the deal, from the
            Currency Definition Table.
          minLength: 3
          maxLength: 3
          example: USD
        dealValue:
          type: number
          description: The value of the deal.
          minimum: 0
        guaranteeCommencementDate:
          format: date
          type: string
          description: >-
            Deal effective date is not in workflow. Currently operations assume
            a date earlier than the Facility effective date. Use the earliest
            Effective Date on facilities in Workflow under this Deal, i.e.
            Guarantee Commencement Date.
        obligorPartyIdentifier:
          type: string
          description: The obligor party identifier.
          minLength: 8
          maxLength: 8
          example: '00000001'
        obligorName:
          type: string
          description: The obligor party name, which is party name1 attribute.
          minLength: 0
          maxLength: 35
        obligorIndustryClassification:
          type: string
          description: The obligor party industry classification.
          minLength: 0
          maxLength: 10
          example: '1405'
      required:
        - dealIdentifier
        - currency
        - dealValue
        - guaranteeCommencementDate
        - obligorPartyIdentifier
        - obligorName
        - obligorIndustryClassification
    CreateDealResponse:
      type: object
      properties:
        dealIdentifier:
          type: string
          example: '0020900035'
      required:
        - dealIdentifier
    CreateDealGuaranteeRequestItem:
      type: object
      properties:
        dealIdentifier:
          type: string
          description: The identifier of the deal to create the guarantee for.
          minLength: 10
          maxLength: 10
          pattern: ^00\\d{8}$
          example: '0030000321'
        effectiveDate:
          format: date
          type: string
          description: >-
            The date that this guarantee will take effect. This will be replaced
            by today's date if a date in the future is provided.
        limitKey:
          type: string
          description: An ACBS party identifier.
          minLength: 8
          maxLength: 8
          example: '00001234'
        guaranteeExpiryDate:
          format: date
          type: string
          description: The date that this guarantee will expire on.
        maximumLiability:
          type: number
          description: The maximum amount the guarantor will guarantee.
          minimum: 0
        guarantorParty:
          type: string
          description: >-
            The party identifier of the guarantor, the customer who is making
            the guarantee/obligation.
          minLength: 8
          maxLength: 8
          default: '00000141'
        guaranteeTypeCode:
          type: string
          description: The identifier for the type of the guarantee.
          minLength: 3
          maxLength: 3
          default: '450'
      required:
        - dealIdentifier
        - effectiveDate
        - limitKey
        - guaranteeExpiryDate
        - maximumLiability
    CreateDealGuaranteeResponse:
      type: object
      properties:
        dealIdentifier:
          type: string
          readOnly: true
          example: '00000001'
      required:
        - dealIdentifier
    GetDealGuaranteeResponseItem:
      type: object
      properties:
        portfolioIdentifier:
          type: string
          example: E1
        dealIdentifier:
          type: string
          example: '0030000321'
        effectiveDate:
          format: date
          type: string
          description: The date that this guarantee will take effect.
        guarantorParty:
          type: string
          description: >-
            The customer identifier of the Guarantor; the Customer who is making
            the guarantee/obligation. This field cannot be updated
          minLength: 8
          maxLength: 8
          example: '00001234'
        limitKey:
          type: string
          description: An ACBS party identifier.
          minLength: 8
          maxLength: 8
          example: '00001234'
        guaranteeExpiryDate:
          format: date
          type: string
          description: The date that this guarantee will expire on.
        maximumLiability:
          type: number
          description: The maximum amount the guarantor will guarantee.
          minimum: 0
          example: 501927.25
        guaranteeTypeCode:
          type: string
          description: >-
            Identifies the type of guarantee provided. The value passed for this
            parameter is validated against the key values in T1080.
          minLength: 1
          example: '450'
      required:
        - portfolioIdentifier
        - dealIdentifier
        - effectiveDate
        - guarantorParty
        - limitKey
        - guaranteeExpiryDate
        - maximumLiability
        - guaranteeTypeCode
    GetDealInvestorDtoLenderType:
      type: object
      properties:
        LenderTypeCode:
          type: string
          description: 'Lender type: 100 for Exporter or 500 for UKEF record.'
          example: '500'
      required:
        - LenderTypeCode
    GetDealInvestorResponseDto:
      type: object
      properties:
        dealIdentifier:
          type: string
          readOnly: true
          example: '0030000321'
        portfolioIdentifier:
          type: string
          readOnly: true
          example: E1
        lenderType:
          readOnly: true
          allOf:
            - $ref: '#/components/schemas/GetDealInvestorDtoLenderType'
        effectiveDate:
          format: date-time
          type: string
          nullable: false
        expiryDate:
          format: date-time
          type: string
          nullable: true
        isExpiryDateMaximum:
          type: boolean
          readOnly: true
          example: false
        maximumLiability:
          type: number
          readOnly: true
          example: 500012.25
      required:
        - dealIdentifier
        - portfolioIdentifier
        - lenderType
        - effectiveDate
        - expiryDate
        - isExpiryDateMaximum
        - maximumLiability
    CreateDealInvestorRequestItem:
      type: object
      properties:
        dealIdentifier:
          type: string
          description: >-
            'The identifier of the deal to create the investor for. It will be a
            10-digit code beginning with either '0020', '0030', or '0040'.
          minLength: 10
          maxLength: 10
          pattern: ^00\\d{8}$
          example: '0020900111'
        lenderType:
          type: string
          description: The lender type code for the investor party of the deal.
          minLength: 0
          maxLength: 3
          example: '500'
          default: '500'
        effectiveDate:
          format: date
          type: string
          description: >-
            The effective date on the deal investor record. If the date provided
            is in the future, it will be replaced by today's date.
          example: '2023-03-24'
        expiryDate:
          format: date
          type: string
          description: >-
            The expiry date on the deal investor record. If the value is not
            provided or is null then the maximum expiry date will be set.
          example: '2023-03-24'
          default: null
          nullable: true
        dealStatus:
          type: string
          description: >-
            A code for the status of the deal on which the sub-limit is created,
            e.g., 'A' for 'ACTIVE/PENDING', 'B' for 'ACTIVE' etc.
          minLength: 0
          maxLength: 1
          example: A
          default: A
        currency:
          type: string
          description: The deal currency code.
          minLength: 3
          maxLength: 3
          example: USD
      required:
        - dealIdentifier
        - effectiveDate
        - currency
    CreateFacilityActivationTransactionRequestItem:
      type: object
      properties:
        facilityIdentifier:
          type: string
          description: The identifier of the facility to activate.
          minLength: 10
          maxLength: 10
          pattern: ^00\\d{8}$
          example: '0030000322'
        initialBundleStatusCode:
          type: number
          description: In most situations the value should be 3, it means auto approval.
          example: 3
          enum:
            - 1
            - 2
            - 3
        lenderTypeCode:
          type: string
          description: >-
            In most situations the value should be 100, it means first level
            obligor.
          minLength: 3
          maxLength: 3
          enum:
            - '100'
            - '500'
          example: '100'
      required:
        - facilityIdentifier
        - initialBundleStatusCode
        - lenderTypeCode
    CreateFacilityActivationTransactionResponse:
      type: object
      properties:
        bundleIdentifier:
          type: string
          minLength: 10
          maxLength: 10
          description: The identifier of the ACBS bundle.
          example: '0000321000'
      required:
        - bundleIdentifier
    GetFacilityActivationTransactionResponseDto:
      type: object
      properties:
        portfolioIdentifier:
          type: string
          description: The identifier of the portfolio in ACBS. The UKEF portfolio is E1.
          minLength: 0
          maxLength: 2
          example: E1
        facilityIdentifier:
          type: string
          description: The identifier of the facility in ACBS.
          minLength: 10
          maxLength: 10
          example: '0030000322'
        bundleStatusCode:
          type: string
          description: A numeric code denoting the status of the bundle.
          minLength: 0
          maxLength: 2
          enum:
            - '10'
            - '15'
            - '20'
            - '25'
            - '30'
            - '35'
            - '40'
            - '50'
            - '55'
            - '80'
            - '85'
            - '90'
          example: '10'
        bundleStatusDesc:
          type: string
          description: >-
            A description of the status of the bundle corresponding to the
            bundle status code.
          minLength: 0
          maxLength: 20
          example: Pending Input With Drafts
        lenderTypeCode:
          type: string
          description: >-
            In most situations the value should be 100, it means first level
            obligor.
          example: '100'
          minLength: 3
          maxLength: 3
          enum:
            - '100'
            - '500'
        initialBundleStatusCode:
          type: number
          description: In most situations the value should be 3, it means auto approval.
          example: 3
          enum:
            - 1
            - 2
            - 3
        initiatingUserName:
          type: string
          description: A value used to populate the Bundle Properties 'Initiated By' field.
          minLength: 0
          maxLength: 60
          example: user1
        accountOwnerIdentifier:
          type: string
          description: The identifier of the account owner in ACBS.
          minLength: 8
          maxLength: 8
          example: '00000000'
        effectiveDate:
          format: date
          type: string
          description: The effective date of the facility.
        facilityTransactionCodeValueCode:
          type: string
          description: '''A'' for ''Active'' to activate facility or ''C'' to cancel.'
          minLength: 0
          maxLength: 10
          example: A
        facilityTransactionTypeCode:
          type: number
          description: >-
            A numeric code denoting the credit arrangement payment transaction
            type.
          minimum: 0
          maximum: 9999
          example: A
        isDraftIndicator:
          type: boolean
          description: If this value is true, it sets the bundle to draft.
        limitKeyValue:
          type: string
          description: The party identifier of the obligor.
          minLength: 0
          maxLength: 8
          example: '00001234'
        limitTypeCode:
          type: string
          description: >-
            A numeric code denoting the type of limit established for the
            involved fee. '00' is 'overall limit'.
          minLength: 0
          maxLength: 2
          enum:
            - '00'
          example: '00'
        sectionIdentifier:
          type: string
          description: >-
            The section identifier to which the limit is associated. For
            example, if the limit is created under the overall facility, this
            value is '00'; if created under a section, this is the ID of that
            section.
          minLength: 0
          maxLength: 2
          example: '00'
      required:
        - portfolioIdentifier
        - facilityIdentifier
        - bundleStatusCode
        - bundleStatusDesc
        - lenderTypeCode
        - initialBundleStatusCode
    GetFacilityByIdentifierResponseDto:
      type: object
      properties:
        dealIdentifier:
          type: string
          readOnly: true
        facilityIdentifier:
          type: string
          readOnly: true
        portfolioIdentifier:
          type: string
          readOnly: true
        dealBorrowerIdentifier:
          type: string
          readOnly: true
        maximumLiability:
          type: number
          readOnly: true
        productTypeId:
          type: string
          readOnly: true
        capitalConversionFactorCode:
          type: string
          readOnly: true
        currency:
          type: string
          readOnly: true
        guaranteeCommencementDate:
          format: date
          type: string
          readOnly: true
          nullable: false
        guaranteeExpiryDate:
          format: date
          type: string
          readOnly: true
          nullable: false
        nextQuarterEndDate:
          format: date
          type: string
          readOnly: true
          nullable: false
        facilityInitialStatus:
          type: string
          readOnly: true
        facilityOverallStatus:
          type: string
          readOnly: true
        delegationType:
          type: string
          readOnly: true
        interestOrFeeRate:
