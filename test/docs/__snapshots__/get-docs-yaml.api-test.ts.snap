// Jest Snapshot v1, https://goo.gl/fbAQLP

exports[`GET /openapi/yaml matches the snapshot 1`] = `
"openapi: 3.0.0
paths:
  /api/v1/deals/{dealIdentifier}:
    get:
      operationId: DealController_getDealByIdentifier
      summary: Get the deal matching the specified deal identifier.
      parameters:
        - name: dealIdentifier
          required: true
          in: path
          description: The identifier of the deal in ACBS.
          example: '0030000321'
          schema:
            type: string
      responses:
        '200':
          description: The deal has been successfully retrieved.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/GetDealByIdentifierResponse'
        '404':
          description: The specified deal was not found.
        '500':
          description: An internal server error has occurred.
  /api/v1/deals:
    post:
      operationId: DealController_createDeal
      summary: Creates a new deal.
      parameters: []
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: array
              items:
                $ref: '#/components/schemas/CreateDealRequestItem'
      responses:
        '201':
          description: The deal has been successfully created.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CreateDealResponse'
        '400':
          description: Bad request.
        '500':
          description: An internal server error has occurred.
  /api/v1/deals/{dealIdentifier}/guarantees:
    post:
      operationId: DealGuaranteeController_createGuaranteeForDeal
      summary: Create a new guarantee for a deal.
      parameters:
        - name: dealIdentifier
          required: true
          in: path
          description: The identifier of the deal in ACBS.
          example: '0030000321'
          schema:
            type: string
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: array
              items:
                $ref: '#/components/schemas/CreateDealGuaranteeRequestItem'
      responses:
        '201':
          description: The guarantee has been successfully created.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CreateDealGuaranteeResponse'
        '400':
          description: Bad request.
        '404':
          description: The deal was not found.
        '500':
          description: An internal server error has occurred.
    get:
      operationId: DealGuaranteeController_getGuaranteesForDeal
      summary: Get all guarantees for a specific deal.
      parameters:
        - name: dealIdentifier
          required: true
          in: path
          description: The identifier of the deal in ACBS
          example: '0030000321'
          schema:
            type: string
      responses:
        '200':
          description: The deal guarantees have been successfully retrieved.
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/GetDealGuaranteeResponseItem'
        '400':
          description: Bad request.
        '404':
          description: The deal was not found.
        '500':
          description: An internal server error has occurred.
  /api/v1/deals/{dealIdentifier}/investors:
    get:
      operationId: DealInvestorController_getDealInvestors
      summary: Get all investors of specific deal.
      parameters:
        - name: dealIdentifier
          required: true
          in: path
          description: UKEF id for deal
          example: '0030000321'
          schema:
            type: string
      responses:
        '200':
          description: The deal investors have been successfully retrieved.
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/GetDealInvestorResponseDto'
        '404':
          description: The specified deal, or the investors for that deal, were not found.
        '500':
          description: An internal server error has occurred.
    post:
      operationId: DealInvestorController_createInvestorForDeal
      summary: Create a new investor for a deal.
      parameters:
        - name: dealIdentifier
          required: true
          in: path
          description: The identifier of the deal in ACBS.
          example: '0020900111'
          schema:
            type: string
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: array
              items:
                $ref: '#/components/schemas/CreateDealInvestorRequestItem'
      responses:
        '201':
          description: The investor has been successfully created.
        '400':
          description: Bad request.
        '404':
          description: The deal was not found.
        '500':
          description: An internal server error has occurred.
  /api/v1/facilities/{facilityIdentifier}:
    get:
      operationId: FacilityController_getFacilityByIdentifier
      summary: Get the facility matching the specified facility identifier.
      parameters:
        - name: facilityIdentifier
          required: true
          in: path
          description: The identifier of the facility in ACBS.
          example: '0030000322'
          schema:
            minLength: 10
            maxLength: 10
            pattern: ^\\d{10}$
            type: string
      responses:
        '200':
          description: The facility has been successfully retrieved.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/GetFacilityByIdentifierResponseDto'
        '404':
          description: The specified facility was not found.
        '500':
          description: An internal server error has occurred.
  /api/v1/facilities:
    post:
      operationId: FacilityController_createFacility
      summary: Create a new facility.
      parameters: []
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: array
              items:
                $ref: '#/components/schemas/CreateFacilityRequestItem'
      responses:
        '201':
          description: The facility has been successfully created.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CreateFacilityResponse'
        '400':
          description: Bad request.
        '500':
          description: An internal server error has occurred.
  /api/v1/facilities/{facilityIdentifier}/covenants:
    get:
      operationId: FacilityCovenantController_getCovenantsForFacility
      summary: Get all covenants for a facility.
      parameters:
        - name: facilityIdentifier
          required: true
          in: path
          description: The identifier of the facility in ACBS.
          example: '0030000322'
          schema:
            minLength: 10
            maxLength: 10
            pattern: ^00\\d{8}$
            type: string
      responses:
        '200':
          description: >-
            The covenants for the facility have been retrieved. Due to
            limitations of ACBS, there is no 404 response; instead, an empty
            array means that either the facility does not exist or it has no
            covenants.
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/GetFacilityCovenantsResponseDto'
        '400':
          description: The specified facilityIdentifier is not valid.
        '500':
          description: An internal server error has occurred.
  /api/v1/facilities/{facilityIdentifier}/guarantees:
    get:
      operationId: FacilityGuaranteeController_getGuaranteesForFacility
      summary: Get all guarantees for a facility.
      parameters:
        - name: facilityIdentifier
          required: true
          in: path
          description: The identifier of the facility in ACBS.
          example: '0030000322'
          schema:
            minLength: 10
            maxLength: 10
            pattern: ^00\\d{8}$
            type: string
      responses:
        '200':
          description: The guarantees for the facility have been successfully retrieved.
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/GetFacilityGuaranteesResponseItem'
        '400':
          description: The specified facilityIdentifier is not valid.
        '404':
          description: >-
            The specified facility, or the guarantees for that facility, were
            not found. (Due to limitations of ACBS, a 404 response does not
            guarantee that the facility does not exist.)
        '500':
          description: An internal server error has occurred.
  /api/v1/facilities/{facilityIdentifier}/investors:
    post:
      operationId: FacilityInvestorController_createInvestorForFacility
      summary: Create a new investor for a facility.
      parameters:
        - name: facilityIdentifier
          required: true
          in: path
          description: The identifier of the facility in ACBS.
          example: '0030000322'
          schema:
            type: string
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: array
              items:
                $ref: '#/components/schemas/CreateFacilityInvestorRequestItem'
      responses:
        '201':
          description: The investor has been successfully created.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CreateFacilityInvestorResponse'
        '400':
          description: Bad request.
        '404':
          description: The facility was not found.
        '500':
          description: An internal server error has occurred.
    get:
      operationId: FacilityInvestorController_getInvestorsForFacility
      summary: Get all investors of the facility.
      parameters:
        - name: facilityIdentifier
          required: true
          in: path
          description: The identifier of the facility in ACBS.
          example: '0030000322'
          schema:
            minLength: 10
            maxLength: 10
            pattern: ^00\\d{8}$
            type: string
      responses:
        '200':
          description: The facility investors have been successfully retrieved.
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/GetFacilityInvestorResponseItem'
        '400':
          description: Bad request.
        '404':
          description: The facility was not found.
        '500':
          description: An internal server error has occurred.
  /api/v1/parties/{partyIdentifier}/external-ratings:
    get:
      operationId: PartyExternalRatingController_getExternalRatingsForParty
      summary: Get all external ratings for a party.
      parameters:
        - name: partyIdentifier
          required: true
          in: path
          description: The identifier of the party in ACBS.
          example: '00000001'
          schema:
            type: string
      responses:
        '200':
          description: The external ratings for the party have been successfully retrieved.
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/GetPartyExternalRatingsResponseItem'
        '404':
          description: >-
            The specified party, or the external ratings for that party, were
            not found.
        '500':
          description: An internal server error has occurred.
  /api/v1/parties:
    get:
      operationId: PartyController_getPartiesBySearchText
      summary: Get all parties matching the specified search text.
      parameters:
        - name: searchText
          required: true
          in: query
          description: 'Minimum length: 3. Cannot contain only whitespaces.'
          schema:
            minLength: 3
            type: string
      responses:
        '200':
          description: The matching parties have been successfully retrieved.
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/GetPartiesBySearchTextResponseItem'
        '500':
          description: An internal server error has occurred.
    post:
      operationId: PartyController_createParty
      summary: >-
        Create a new party if there are no parties matching this alternate
        identifier.
      parameters: []
      requestBody:
        required: true
        content:
          application/json:
            schema:
              type: array
              items:
                $ref: '#/components/schemas/CreatePartyRequestItem'
      responses:
        '200':
          description: >-
            One or more parties matching this alternate identifier already
            exist.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CreatePartyResponse'
        '201':
          description: The party has been successfully created.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CreatePartyResponse'
        '400':
          description: Bad request.
        '500':
          description: An internal server error has occurred.
  /api/v1/parties/{partyIdentifier}:
    get:
      operationId: PartyController_getPartyByIdentifier
      summary: Get the party matching the specified party identifier.
      parameters:
        - name: partyIdentifier
          required: true
          in: path
          description: The identifier of the party in ACBS.
          example: '00000001'
          schema:
            type: string
      responses:
        '200':
          description: The party has been successfully retrieved.
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/GetPartyByIdentifierResponse'
        '404':
          description: The specified party was not found.
        '500':
          description: An internal server error has occurred.
info:
  title: TFS API Specification
  description: TFS API documentation
  version: '1.0'
  contact: {}
tags: []
servers: []
components:
  securitySchemes:
    ApiKeyHeader:
      type: apiKey
      in: header
      name: x-api-key
  schemas:
    GetDealByIdentifierResponse:
      type: object
      properties:
        dealIdentifier:
          type: string
          readOnly: true
          example: '0030000321'
        portfolioIdentifier:
          type: string
          readOnly: true
          example: E1
        currency:
          type: string
          readOnly: true
          example: USD
        dealValue:
          type: number
          readOnly: true
        guaranteeCommencementDate:
          format: date
          type: string
          readOnly: true
        obligorPartyIdentifier:
          type: string
          readOnly: true
          example: '00001234'
        obligorName:
          type: string
          readOnly: true
        obligorIndustryClassification:
          type: string
          readOnly: true
      required:
        - dealIdentifier
        - portfolioIdentifier
        - currency
        - dealValue
        - guaranteeCommencementDate
        - obligorPartyIdentifier
        - obligorName
        - obligorIndustryClassification
    CreateDealRequestItem:
      type: object
      properties:
        dealIdentifier:
          type: string
          description: The identifier of the deal to create.
          minLength: 10
          maxLength: 10
          example: '0020900035'
        currency:
          type: string
          description: >-
            The currency code of the primary currency of the deal, from the
            Currency Definition Table.
          minLength: 3
          maxLength: 3
          example: USD
        dealValue:
          type: number
          description: The value of the deal.
          minimum: 0
        guaranteeCommencementDate:
          format: date
          type: string
          description: >-
            Deal effective date is not in workflow. Currently operations assume
            a date earlier than the Facility effective date. Use the earliest
            Effective Date on facilities in Workflow under this Deal, i.e.
            Guarantee Commencement Date.
        obligorPartyIdentifier:
          type: string
          description: The obligor party identifier.
          minLength: 8
          maxLength: 8
          example: '00000001'
        obligorName:
          type: string
          description: The obligor party name, which is party name1 attribute.
          minLength: 0
          maxLength: 35
        obligorIndustryClassification:
          type: string
          description: The obligor party industry classification.
          minLength: 0
          maxLength: 10
          example: '1405'
      required:
        - dealIdentifier
        - currency
        - dealValue
        - guaranteeCommencementDate
        - obligorPartyIdentifier
        - obligorName
        - obligorIndustryClassification
    CreateDealResponse:
      type: object
      properties:
        dealIdentifier:
          type: string
          example: '0020900035'
      required:
        - dealIdentifier
    CreateDealGuaranteeRequestItem:
      type: object
      properties:
        dealIdentifier:
          type: string
          description: The identifier of the deal to create the guarantee for.
          minLength: 8
          maxLength: 8
          example: '00000001'
        effectiveDate:
          format: date
          type: string
          description: >-
            The date that this guarantee will take effect. This will be replaced
            by today's date if a date in the past is provided.
        limitKey:
          type: string
          description: An ACBS party identifier.
          minLength: 8
          maxLength: 8
          example: '00000002'
        guaranteeExpiryDate:
          format: date
          type: string
          description: The date that this guarantee will expire on.
        maximumLiability:
          type: number
          description: The maximum amount the guarantor will guarantee.
          minimum: 0
        guarantorParty:
          type: string
          description: >-
            The party identifier of the guarantor, the customer who is making
            the guarantee/obligation.
          minLength: 8
          maxLength: 8
          default: '00000141'
        guaranteeTypeCode:
          type: string
          description: The identifier for the type of the guarantee.
          minLength: 3
          maxLength: 3
          default: '450'
      required:
        - dealIdentifier
        - effectiveDate
        - limitKey
        - guaranteeExpiryDate
        - maximumLiability
    CreateDealGuaranteeResponse:
      type: object
      properties:
        dealIdentifier:
          type: string
          readOnly: true
          example: '00000001'
      required:
        - dealIdentifier
    GetDealGuaranteeResponseItem:
      type: object
      properties:
        portfolioIdentifier:
          type: string
          example: E1
        dealIdentifier:
          type: string
          example: '0030000321'
        effectiveDate:
          format: date
          type: string
          description: The date that this guarantee will take effect.
        guarantorParty:
          type: string
          description: >-
            The customer identifier of the Guarantor; the Customer who is making
            the guarantee/obligation. This field cannot be updated
          minLength: 8
          maxLength: 8
          example: '00001234'
        limitKey:
          type: string
          description: An ACBS party identifier.
          minLength: 8
          maxLength: 8
          example: '00001234'
        guaranteeExpiryDate:
          format: date
          type: string
          description: The date that this guarantee will expire on.
        maximumLiability:
          type: number
          description: The maximum amount the guarantor will guarantee.
          minimum: 0
          example: 501927.25
        guaranteeTypeCode:
          type: string
          description: >-
            Identifies the type of guarantee provided. The value passed for this
            parameter is validated against the key values in T1080.
          minLength: 1
          example: '450'
      required:
        - portfolioIdentifier
        - dealIdentifier
        - effectiveDate
        - guarantorParty
        - limitKey
        - guaranteeExpiryDate
        - maximumLiability
        - guaranteeTypeCode
    GetDealInvestorDtoLenderType:
      type: object
      properties:
        LenderTypeCode:
          type: string
          description: 'Lender type: 100 for Exporter or 500 for UKEF record.'
          example: '500'
      required:
        - LenderTypeCode
    GetDealInvestorResponseDto:
      type: object
      properties:
        dealIdentifier:
          type: string
          readOnly: true
          example: '0030000321'
        portfolioIdentifier:
          type: string
          readOnly: true
          example: E1
        lenderType:
          readOnly: true
          allOf:
            - $ref: '#/components/schemas/GetDealInvestorDtoLenderType'
        effectiveDate:
          format: date-time
          type: string
          nullable: false
        expiryDate:
          format: date-time
          type: string
          nullable: true
        isExpiryDateMaximum:
          type: boolean
          readOnly: true
          example: false
        maximumLiability:
          type: number
          readOnly: true
          example: 500012.25
      required:
        - dealIdentifier
        - portfolioIdentifier
        - lenderType
        - effectiveDate
        - expiryDate
        - isExpiryDateMaximum
        - maximumLiability
    CreateDealInvestorRequestItem:
      type: object
      properties:
        dealIdentifier:
          type: string
          description: >-
            'The identifier of the deal to create the investor for. It will be a
            10-digit code beginning with either '0020', '0030', or '0040'.
          minLength: 10
          maxLength: 10
          pattern: ^00\\d{8}$
          example: '0020900111'
        lenderType:
          type: string
          description: The lender type code for the investor party of the deal.
          minLength: 0
          maxLength: 3
          example: '500'
          default: '500'
        effectiveDate:
          format: date
          type: string
          description: >-
            The effective date on the deal investor record. If the date provided
            is in the future, it will be replaced by today's date.
          example: '2023-03-24'
        expiryDate:
          format: date
          type: string
          description: >-
            The expiry date on the deal investor record. If the value is not
            provided or is null then the maximum expiry date will be set.
          example: '2023-03-24'
          default: null
          nullable: true
        dealStatus:
          type: string
          description: >-
            A code for the status of the deal on which the sub-limit is created,
            e.g., 'A' for 'ACTIVE/PENDING', 'B' for 'ACTIVE' etc.
          minLength: 0
          maxLength: 1
          example: A
          default: A
        currency:
          type: string
          description: The deal currency code.
          minLength: 3
          maxLength: 3
          example: USD
      required:
        - dealIdentifier
        - effectiveDate
        - currency
    GetFacilityByIdentifierResponseDto:
      type: object
      properties:
        dealIdentifier:
          type: string
          readOnly: true
        facilityIdentifier:
          type: string
          readOnly: true
        portfolioIdentifier:
          type: string
          readOnly: true
        dealBorrowerIdentifier:
          type: string
          readOnly: true
        maximumLiability:
          type: number
          readOnly: true
        productTypeId:
          type: string
          readOnly: true
        capitalConversionFactorCode:
          type: string
          readOnly: true
        currency:
          type: string
          readOnly: true
        guaranteeCommencementDate:
          format: date
          type: string
          readOnly: true
          nullable: false
        guaranteeExpiryDate:
          format: date
          type: string
          readOnly: true
          nullable: false
        nextQuarterEndDate:
          format: date
          type: string
          readOnly: true
          nullable: false
        facilityInitialStatus:
          type: string
          readOnly: true
        facilityOverallStatus:
          type: string
          readOnly: true
        delegationType:
          type: string
          readOnly: true
        interestOrFeeRate:
          type: number
          readOnly: true
        facilityStageCode:
          type: string
          readOnly: true
        exposurePeriod:
          type: string
          readOnly: true
        creditRatingCode:
          type: string
          readOnly: true
        guaranteePercentage:
          type: number
          readOnly: true
        premiumFrequencyCode:
          type: string
          readOnly: true
        riskCountryCode:
          type: string
          readOnly: true
        riskStatusCode:
          type: string
          readOnly: true
        effectiveDate:
          format: date
          type: string
          readOnly: true
          nullable: false
        forecastPercentage:
          type: number
          readOnly: true
        issueDate:
          format: date
          type: string
          readOnly: true
          nullable: true
        description:
          type: string
          readOnly: true
        agentBankIdentifier:
          type: string
          readOnly: true
        obligorPartyIdentifier:
          type: string
          readOnly: true
        obligorName:
          type: string
          readOnly: true
        obligorIndustryClassification:
          type: string
          readOnly: true
        probabilityOfDefault:
          type: number
          readOnly: true
      required:
        - dealIdentifier
        - facilityIdentifier
        - portfolioIdentifier
        - dealBorrowerIdentifier
        - maximumLiability
        - productTypeId
        - capitalConversionFactorCode
        - currency
        - guaranteeCommencementDate
        - guaranteeExpiryDate
        - nextQuarterEndDate
        - facilityInitialStatus
        - facilityOverallStatus
        - delegationType
        - interestOrFeeRate
        - facilityStageCode
        - exposurePeriod
        - creditRatingCode
        - guaranteePercentage
        - premiumFrequencyCode
        - riskCountryCode
        - riskStatusCode
        - effectiveDate
        - forecastPercentage
        - issueDate
        - description
        - agentBankIdentifier
        - obligorPartyIdentifier
        - obligorName
        - obligorIndustryClassification
        - probabilityOfDefault
    CreateFacilityRequestItem:
      type: object
      properties:
        dealIdentifier:
          type: string
          description: The identifier of the deal for the facility.
          minLength: 10
          maxLength: 10
          pattern: ^00\\d{8}$
          example: '0030000321'
        facilityIdentifier:
          type: string
          description: The identifier of the facility to create.
          minLength: 10
          maxLength: 10
          pattern: ^00\\d{8}$
          example: '0030000322'
        dealBorrowerIdentifier:
          type: string
          description: >-
            Look up the Obligors ACBS Customer record using Party URN on the
            Deal = Alternate Customer Id (J$MSZI). Use ACBS Customer Id (J$MRUI)
            Note there may be multiple customers in ACBS with the same Party
            URN. Use the first record found.
          minLength: 8
          maxLength: 8
          example: '00001234'
        productTypeId:
          type: string
          description: The facility type, e.g. '250' for BOND.
          minLength: 0
          maxLength: 3
          example: '250'
        productTypeName:
          type: string
          description: The name or description of the facility type.
          minLength: 0
          maxLength: 13
          example: BOND
        exposurePeriod:
          type: string
          description: Credit Period
          minLength: 0
          maxLength: 12
          example: '1'
        currency:
          type: string
          description: Facility Currency Code e.g. CAD, USD.
          minLength: 3
          maxLength: 3
          example: USD
        obligorIndustryClassification:
          type: string
          description: The industry classification code of the obligor.
          minLength: 0
          maxLength: 10
          example: '0116'
        effectiveDate:
          format: date
          type: string
          description: >-
            The date from which the borrower can draw funds from the facility.
            If this is a future date then it will be replaced with today's date.
        guaranteeExpiryDate:
          format: date
          type: string
          description: The expiration date of the facility.
        nextQuarterEndDate:
          format: date
          type: string
          description: The end date of the next quarter.
        maximumLiability:
          type: number
          description: The overall limit for the facility.
          minimum: 0
        agentBankIdentifier:
          type: string
          description: The party identifier of the Agent Bank for this facility.
          minLength: 0
          maxLength: 10
          example: '00000000'
        riskCountryCode:
          type: string
          description: >-
            The code of the primary country of risk designated for this credit
            arrangement.
          minLength: 0
          maxLength: 3
          example: GBR
        premiumFrequencyCode:
          type: string
          description: Pre-Issue Est. Payment Frequency QUARTERLY(2)
          minLength: 0
          maxLength: 1
          example: '2'
        riskStatusCode:
          type: string
          description: CORPORATE(03)
          minLength: 0
          maxLength: 2
          example: '03'
        creditRatingCode:
          type: string
          description: Credit Review Risk Code
          minLength: 0
          maxLength: 2
          example: '13'
        facilityStageCode:
          type: string
          description: Case Stage this can be 06 Commitment and 07 Issued
          minLength: 0
          maxLength: 2
          example: '07'
        delegationType:
          type: string
          description: Derive values A, M or N
          minLength: 0
          maxLength: 4
          example: A
        interestOrFeeRate:
          type: number
          description: >-
            Bank Rate, this can be for Bond facility corresponding fee rate or
            for Loan/EWCS interest rate.
          minimum: 0
        obligorPartyIdentifier:
          type: string
          description: The party identifier of the obligor.
          minLength: 8
          maxLength: 8
          example: '00001234'
        forecastPercentage:
          type: number
          description: Forecast % Derive from FACILITY:Stage, i.e. Commitment or Issued
          minimum: 0
        probabilityOfDefault:
          type: number
          description: >-
            A percentage indicating how likely it is that a default on the
            credit will occur. This should be specified for GEF.
          minimum: 0
          default: 0
        capitalConversionFactorCode:
          type: string
          description: >-
            The code of the Capital Conversion Factor for the facility (e.g.
            100% Conversion, 80% Conversion, No Conversion) from the T1125 table
            in ACBS. This field should be specified for GEF. When this field is
            not specified, a default will be chosen based on the productTypeId.
          minLength: 0
          maxLength: 2
          example: '1'
        issueDate:
          format: date
          type: string
          description: >-
            At Commitment stage its not required, Issued when issued it would be
            Issue Date for Bond OR Disbursement Date for Loan/EWCS.
          default: null
          nullable: true
      required:
        - dealIdentifier
        - facilityIdentifier
        - dealBorrowerIdentifier
        - productTypeId
        - productTypeName
        - exposurePeriod
        - currency
        - obligorIndustryClassification
        - effectiveDate
        - guaranteeExpiryDate
        - nextQuarterEndDate
        - maximumLiability
        - agentBankIdentifier
        - riskCountryCode
        - premiumFrequencyCode
        - riskStatusCode
        - creditRatingCode
        - facilityStageCode
        - delegationType
        - interestOrFeeRate
        - obligorPartyIdentifier
        - forecastPercentage
